<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="275.0" spacing="10.0" stylesheets="@../styles/application.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.CatRow">
   <children>
      <ImageView fx:id="picture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
      <HBox style="-fx-alignment: center;" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="catname" text="Label">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox spacing="20.0" style="-fx-alignment: center;">
         <children>
            <Button mnemonicParsing="false" onAction="#delete" styleClass="delete" text="Delete" />
            <Button mnemonicParsing="false" onAction="#update" styleClass="edit" text="Edit" />
         </children>
      </HBox>
   </children>
</HBox>
